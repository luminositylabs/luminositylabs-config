                <ignoreVersion type="regex">1\.4\.[0-5]</ignoreVersion>
                <ignoreVersion type="regex">1\.3\.[0-5]</ignoreVersion>
                <ignoreVersion type="regex">1\.2\..*</ignoreVersion>
            </ignoreVersions>
        </rule>

